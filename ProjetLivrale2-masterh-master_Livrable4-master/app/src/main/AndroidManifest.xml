        <activity android:name=".ServiceList2" />
        <activity
            android:name=".FournisseurList"
            android:label="@string/title_activity_fournisseur_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".FournisseurListForEachService" />
        <activity android:name=".RatingService" />
